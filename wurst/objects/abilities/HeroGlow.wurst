package HeroGlow
import AbilityObjEditing
import LocalObjectIDs

@compiletime function createTeamGlowAbility()
    new AbilityDefinitionAuraRegenerationHealingWard(ABILITY_HERO_GLOW)
    ..setName("Hero Glow")
    ..setArtTarget("Models\\GeneralHeroGlow.mdl")
    ..setEditorSuffix("(wurst)")
    ..setAmountofHitPointsRegenerated(1, 0)
    ..setBuffs(1, "")
    ..setAreaofEffect(1, 1)
    ..setTargetAttachmentPoint("origin")
