package GroupUtils

let trolls = CreateGroup()

public function getTrolls() returns group
    return trolls

public function removeTroll(unit troll)
    print("removing troll" + (troll == null ? "null" : troll.getName()))
    trolls.removeUnit(troll)
    print("removed troll")

public function addTroll(unit troll)
    print("adding troll" + (troll == null ? "null" : troll.getName()))
    trolls.addUnit(troll)
    print("added troll")
