package UnitPoolExtensions

public function unitpool.addUnitType(int unitId, real weight)
    UnitPoolAddUnitType(this, unitId, weight)

public function unitpool.removeUnitType(int unitId)
    UnitPoolRemoveUnitType(this, unitId)

public function unitpool.placeRandomUnit(player whichPlayer, vec2 pos, angle facing) returns unit
    return PlaceRandomUnit(this, whichPlayer, pos.x, pos.y, facing.degrees())
