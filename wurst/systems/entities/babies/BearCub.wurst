package BearCub
import LocalObjectIDs
import BabyAnimal

public class BearCub extends BabyAnimal
    private static let unitTypeId = UNIT_BEAR_CUB

    static function initialize()
        register(unitTypeId, cub -> new BearCub(cub))

    construct(unit whichUnit)
        super(whichUnit)

init
    BearCub.initialize()
