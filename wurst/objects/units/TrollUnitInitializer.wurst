function createHunter(int unitId) returns HeroDefinition
    return createBasicTroll(unitId)
        ..setNameEnhance("Hunter")
        ..setAttackDamage(13)
        ..setAttackSpeed(1.75)
        ..setModelFile(Units.forestTrollTrapper1)
        ..setIcons(Icons.bTNForestTroll)
        ..setUnitSoundSet(Soundsets.forestTrollShadowPriest)
        ..setSpeedBase(300)
        ..setButtonPositionX(1)
        ..setButtonPositionY(1)
        ..setScalingValue(1)
        ..setSightRadiusDay(1000)
        ..setSightRadiusNight(500)
        ..setTooltipExtended(TOOLTIP_HUNTER)
        ..setProperNames(NAME_HUNTER)
        ..setProperNamesUsed(4)
        ..setHotkey("W")
        ..setAbilitiesEnhance(NORMAL_SPELLS_HUNTER, HERO_SPELLS_HUNTER)

@compiletime function createHunter()
    createHunter(UNIT_HUNTER)
    createHunter(UNIT_HUNTER_1)
        ..setModelFile(LocalUnits.axeTroll)
        ..setNameEditorSuffix("(Custom Skin 1)")


