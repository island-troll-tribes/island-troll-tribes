package LocalSoundUtils

// Standard library imports:
import Assets

public function playMammothHorn()
    // Create, play, and kill the sound.
    KillSoundWhenDone(
        CreateSound(
            Sounds.theHornOfCenarius,
            false,
            false,
            false,
            10,
            10,
            ""
        )
            ..setPitch(1.)
            ..setVolume(127)
            ..play()
    )
