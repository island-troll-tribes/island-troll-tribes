package CureAllItem
import InstantDummyCaster
import RegisterEvents
import LocalObjectIDs


init
    registerSpellEffectEvent(SPELL_CURE_ALL_ITEM) ->
        let target = GetSpellTargetUnit()
        let owner = target.getOwner()
        let pos = target.getPos()
        print("going to cast the real cure all")
        InstantDummyCaster.castTarget(owner, SPELL_CURE_ALL, 1, "autodispel", target)
        print("finished forwarding the cast")
