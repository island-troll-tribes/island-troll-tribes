package TradeShipRoutePair
import TradeShipStop

public class TradeShipRoutePair
    TradeShipStop a
    TradeShipStop b

    construct(TradeShipStop _a, TradeShipStop _b)
        a = _a
        b = _b

    function swap()
        let tmp = a
        a = b
        b = tmp

    function getFirst() returns TradeShipStop
        return a

    function getLast() returns TradeShipStop
        return b
