package InitializeHerbs

init
    udg_plants = GetUnitsInRectMatching(GetWorldBounds(), Filter(() -> begin
        return GetFilterUnit().isType(UNIT_TYPE_ANCIENT)
    end))

    for bush in udg_plants
        if bush.getOwner().getId() == PLAYER_NEUTRAL_AGGRESSIVE
            bush.setOwner(Player(PLAYER_NEUTRAL_PASSIVE), true)
